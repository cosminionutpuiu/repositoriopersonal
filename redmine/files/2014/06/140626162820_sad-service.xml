<?xml version="1.0" encoding="UTF-8"?>
<server>
    <mbean code="org.jboss.naming.JNDIBindingServiceMgr" name="jboss.tests:name=example1">
        <attribute name="BindingsConfig" serialDataType="jbxb">
            <jndi:bindings xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
                xmlns:jndi="urn:jboss:jndi-binding-service:1.0"
                xs:schemaLocation="urn:jboss:jndi-binding-service  \
               resource:jndi-binding-service_1_0.xsd">

                <!-- Identificador único del SAD -->
                <jndi:binding name="sad/sad.uid">
                    <jndi:value type="java.lang.String">sac-cci-test</jndi:value>
                </jndi:binding>
                
                <!-- Versión del SAD -->
                <jndi:binding name="sad/sad.version">
                    <jndi:value type="java.lang.String">2.10.0</jndi:value>
                </jndi:binding>

                <!-- Dirección completa del SAD (UID + dominio) -->
                <jndi:binding name="sad/sad.inetAddress">
                    <jndi:value type="java.lang.String">sac-cci-test.agata.juntadeandalucia.es</jndi:value>
                </jndi:binding>

                <!-- Contexto de despliegue de la aplicación sad-web en jboss -->
                <jndi:binding name="sad/sad.context.sad-web">
                    <jndi:value type="java.lang.String">sad-web</jndi:value>
                </jndi:binding>

                <!-- Contexto de despliegue de la aplicación sad-web-admin en jboss -->
                <jndi:binding name="sad/sad.context.sad-web-admin">
                    <jndi:value type="java.lang.String">sad-web-admin</jndi:value>
                </jndi:binding>

                <!-- Contexto de despliegue de la aplicación sad-dav en jboss -->
                <jndi:binding name="sad/sad.context.sad-dav">
                    <jndi:value type="java.lang.String">sad-dav</jndi:value>
                </jndi:binding>

                <!-- Contexto de despliegue de la aplicación sad-comm en jboss -->
                <jndi:binding name="sad/sad.context.sad-comm">
                    <jndi:value type="java.lang.String">sad-comm</jndi:value>
                </jndi:binding>

                <!-- Propiedades para evitar ataques por fuerza bruta en sad-web y sad-web-admin -->
                <!-- Número máximo de intentos fallidos de acceso a las interfaces web -->
                <jndi:binding name="sad/sad.max.failed.attempts">
                    <jndi:value type="java.lang.String">30</jndi:value>
                </jndi:binding>
                
                <!-- Número de minutos máximo durante el que se tendrá en cuenta el número máximo de accesos fallidos -->
                <jndi:binding name="sad/sad.max.period.failed.attempts">
                    <jndi:value type="java.lang.String">30</jndi:value>
                </jndi:binding>
                
                <!-- Número de minutos que habrá que esperar si se produce el número de accesos fallidos máximo -->
                <jndi:binding name="sad/sad.failed.attempts.wait">
                    <jndi:value type="java.lang.String">30</jndi:value>
                </jndi:binding>

                <!-- Propiedades de Hibernate -->
                <!-- Driver de conexión -->
                <jndi:binding name="sad/sad.hibernate.connection.driver">
                    <jndi:value type="java.lang.String">oracle.jdbc.OracleDriver</jndi:value>
                </jndi:binding>

                <!-- Cadena completa de conexión a BD -->
                <jndi:binding name="sad/sad.hibernate.connection.url">
                    <jndi:value type="java.lang.String">jdbc:oracle:thin:@(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=10.240.202.30)(PORT=1521))(CONNECT_DATA=(SERVER=DEDICATED)(SERVICE_NAME=DESICOP))))</jndi:value>
                </jndi:binding>

                <!-- Usuario para conexión a base de datos -->
                <jndi:binding name="sad/sad.hibernate.connection.username">
                    <jndi:value type="java.lang.String">SAD_WEB</jndi:value>
                </jndi:binding>

                <!-- Contraseña para conexión a base de datos -->
                <jndi:binding name="sad/sad.hibernate.connection.password">
                    <jndi:value type="java.lang.String">SAD_WEB</jndi:value>
                </jndi:binding>

                <!-- Dialecto para la base de datos -->
                <jndi:binding name="sad/sad.hibernate.connection.dialect">
                    <jndi:value type="java.lang.String">org.hibernate.dialect.Oracle10gDialect</jndi:value>
                </jndi:binding>

                <!-- Propiedad de configuración de hibernate search. Dejar valor indicado. -->
                <jndi:binding name="sad/sad.hibernate.search.default.directory_provider">
                    <jndi:value type="java.lang.String">org.hibernate.search.store.RAMDirectoryProvider</jndi:value>
                </jndi:binding>

                <!-- Propiedad de configuración de hibernate search. Dejar esta propiedad 
                    vacía. -->
                <jndi:binding name="sad/sad.hibernate.search.default.indexBase">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Propiedad de configuración de hibernate para la generación automática 
                    de la base de datos. Dejar esta propiedad vacía. -->
                <jndi:binding name="sad/sad.hibernate.hbm2ddl.auto">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Propiedad de configuración de hibernate con la que se establece 
                    la profundidad máxima a la hora de recuperar elementos dependientes de base 
                    de datos. Dejar siempre a 1 -->
                <jndi:binding name="sad/sad.hibernate.max_fetch_depth">
                    <jndi:value type="java.lang.String">1</jndi:value>
                </jndi:binding>


                <!-- Parámetros del pool de conexiones -->
                <!-- Tamaño inicial del pool -->
                <jndi:binding name="sad/sad.c3p0.initialPoolSize">
                    <jndi:value type="java.lang.String">5</jndi:value>
                </jndi:binding>

                <!-- Tamaño máximo del pool -->
                <jndi:binding name="sad/sad.c3p0.maxPoolSize">
                    <jndi:value type="java.lang.String">35</jndi:value>
                </jndi:binding>

                <!-- Número de hebras auxiliares -->
                <jndi:binding name="sad/sad.c3p0.numHelperThreads">
                    <jndi:value type="java.lang.String">6</jndi:value>
                </jndi:binding>

                <!-- Tiempo máximo de vida de una conexión inactiva -->
                <jndi:binding name="sad/sad.c3p0.maxIdleTime">
                    <jndi:value type="java.lang.String">300</jndi:value>
                </jndi:binding>

                <!-- Tiempo máximo de espera para obtener una conexión -->
                <jndi:binding name="sad/sad.c3p0.checkoutTimeout">
                    <jndi:value type="java.lang.String">30000</jndi:value>
                </jndi:binding>

                <!-- Tiempo para comprobación de conexiones ociosas -->
                <jndi:binding name="sad/sad.c3p0.idleConnectionTestPeriod">
                    <jndi:value type="java.lang.String">100</jndi:value>
                </jndi:binding>

                <!-- Consulta para comprobar la conexión -->
                <jndi:binding name="sad/sad.c3p0.preferredTestQuery">
                    <jndi:value type="java.lang.String">SELECT 1 FROM DUAL</jndi:value>
                </jndi:binding>


                <!-- Configuracion LDAP corporativo -->
                <!-- Url del servidor ldap -->
                <jndi:binding name="sad/ldap.url">
                    <jndi:value type="java.lang.String">ldap://ldap23.juntadeandalucia.es:389</jndi:value>
                </jndi:binding>

                <!-- Base dn -->
                <jndi:binding name="sad/ldap.baseDn">
                    <jndi:value type="java.lang.String">o=juntadeandalucia,c=es</jndi:value>
                </jndi:binding>

                <!-- Dn del usuario para acceso a LDAP -->
                <jndi:binding name="sad/ldap.bindDn">
                    <jndi:value type="java.lang.String">uid=consultaldap.agata,o=general,o=empleados,o=juntadeandalucia,c=es</jndi:value>
                </jndi:binding>

                <!-- Password del usuario para acceso a LDAP -->
                <jndi:binding name="sad/ldap.bindPw">
                    <jndi:value type="java.lang.String">dameeldni</jndi:value>
                </jndi:binding>

                <!-- Rama a partir del base dn, sobre la que realizar las búsquedas -->
                <jndi:binding name="sad/ldap.search.userDn">
                    <jndi:value type="java.lang.String">o=empleados</jndi:value>
                </jndi:binding>

                <!-- Tiempo máximo en milisegundos de espera para obtener los resultados 
                    de una consulta -->
                <jndi:binding name="sad/ldap.time.limit">
                    <jndi:value type="java.lang.String">0</jndi:value>
                </jndi:binding>

                <!-- Número máximo de elementos devueltos en una consulta a ldap -->
                <jndi:binding name="sad/ldap.size.limit">
                    <jndi:value type="java.lang.String">30</jndi:value>
                </jndi:binding>

                <!-- Tamaño de página para consultas paginadas -->
                <jndi:binding name="sad/ldap.page.size">
                    <jndi:value type="java.lang.String">10</jndi:value>
                </jndi:binding>

                <!-- Tiempo máximo en milisegundos de espera para obtener una conexión 
                    a ldap cuando no hay conexiones disponibles -->
                <jndi:binding name="sad/ldap.pool.maxWait">
                    <jndi:value type="java.lang.String">10000</jndi:value>
                </jndi:binding>

                <!-- Tiempo en milisegundos para la comprobación de conexiones ociosas -->
                <jndi:binding name="sad/ldap.pool.timeBetweenEvictionRunsMillis">
                    <jndi:value type="java.lang.String">9000</jndi:value>
                </jndi:binding>

                <!-- Máximo número de conexiones en el pool -->
                <jndi:binding name="sad/ldap.pool.maxActive">
                    <jndi:value type="java.lang.String">25</jndi:value>
                </jndi:binding>

                <!-- Tiempo mínimo en milisegundos para que una conexion se considere 
                    ociosa -->
                <jndi:binding name="sad/ldap.pool.minEvictableIdleTimeMillis">
                    <jndi:value type="java.lang.String">90000</jndi:value>
                </jndi:binding>


                <!-- Propiedades de Funambol -->
                <!-- URL de ws de administración de funambol. -->
                <jndi:binding name="sad/funambol.ws.admin.url">
                    <jndi:value type="java.lang.String">http://10.240.202.23:8180/funambol/services/admin</jndi:value>
                </jndi:binding>

                <!-- Usuario de administración de Funambol. -->
                <jndi:binding name="sad/funambol.admin.user">
                    <jndi:value type="java.lang.String">admin</jndi:value>
                </jndi:binding>

                <!-- Contraseña -->
                <jndi:binding name="sad/funambol.admin.password">
                    <jndi:value type="java.lang.String">123456</jndi:value>
                </jndi:binding>

                <!-- Propiedad que indica si los principales se deben crear automáticamente. 
                    Valores true/false -->
                <jndi:binding name="sad/funambol.autoCreatePrincipals">
                    <jndi:value type="java.lang.String">true</jndi:value>
                </jndi:binding>

                <!-- Url de conexión a BD de Funambol -->
                <jndi:binding name="sad/funambol.db.url">
                    <jndi:value type="java.lang.String">jdbc:oracle:thin:@(DESCRIPTION = (ADDRESS = (PROTOCOL = TCP)(HOST = 10.240.202.30)(PORT = 1521))(CONNECT_DATA = (SERVER = DEDICATED)(SERVICE_NAME = DESICOP))))</jndi:value>
                </jndi:binding>

                <!-- Usuario de esquema de BD -->
                <jndi:binding name="sad/funambol.db.user">
                    <jndi:value type="java.lang.String">FUNAMBOL_OWNER</jndi:value>
                </jndi:binding>

                <!-- Contraseña de esquema de BD -->
                <jndi:binding name="sad/funambol.db.pass">
                    <jndi:value type="java.lang.String">FUNAMBOL_OWNER_DES</jndi:value>
                </jndi:binding>

                <!-- Driver de oracle -->
                <jndi:binding name="sad/funambol.db.driver">
                    <jndi:value type="java.lang.String">oracle.jdbc.OracleDriver</jndi:value>
                </jndi:binding>


                <!-- Propiedades de sad-comm -->
                <!-- URL al servicio web de suscripciones externas del SAD-. Esta petición 
                    debe realizarse vía CSA -->
                <jndi:binding name="sad/sad.comm.subscription.client.ws.url">
                    <jndi:value type="java.lang.String">https://10.240.202.23/soap/proxy/</jndi:value>
                </jndi:binding>

                <!-- URL del proxy HTTP (si existe) que usarán los clientes de los servicios 
                    web de sad-comm -->
                <jndi:binding name="sad/sad.comm.client.proxy">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Puerto de escucha del proxy -->
                <jndi:binding name="sad/sad.comm.client.proxy.port">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Usuario para utilizar dicho proxy -->
                <jndi:binding name="sad/sad.comm.client.proxy.user">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Contraseña de dicho usuario -->
                <jndi:binding name="sad/sad.comm.client.proxy.pass">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Numero máximo de elementos devueltos en las operaciones de sad-comm -->
                <jndi:binding name="sad/sad.comm.max.results">
                    <jndi:value type="java.lang.String">200</jndi:value>
                </jndi:binding>

                <!-- Descripción del módulo sad-comm -->
                <jndi:binding name="sad/sad.comm.uid">
                    <jndi:value type="java.lang.String">Plataforma AG@TAv2 - Sistema de Agenda
                        Distribuido (SAD) - Middleware de comunicación</jndi:value>
                </jndi:binding>


                <!-- Propiedades para los tests -->
                <!-- Estas propiedades se pueden dejar vacias si no se van a ejecutar 
                    los tests -->
                <!-- Usuario para la realización de pruebas de sad-comm y sad-dav -->
                <jndi:binding name="sad/sad.test.user">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Contraseña del usuario anterior -->
                <jndi:binding name="sad/sad.test.user.password">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Usuario al que crear suscripciones -->
                <jndi:binding name="sad/sad.test.subscription.user">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Host de pruebas -->
                <jndi:binding name="sad/sad.test.host">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Puerto del host anterior -->
                <jndi:binding name="sad/sad.test.port">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>


                <!-- Email del remitente de las notificaciones -->
                <jndi:binding name="sad/sad.notification.sender">
                    <jndi:value type="java.lang.String">francisco.giles.ext@juntadeandalucia.es</jndi:value>
                </jndi:binding>

                <!-- Email de contacto -->
                <jndi:binding name="sad/sad.support.email">
                    <jndi:value type="java.lang.String">francisco.giles.ext@juntadeandalucia.es</jndi:value>
                </jndi:binding>

                <!-- Habilita o deshabilita las funcionadlidades asociadas a grupos. 
                    Dejar siempre a false -->
                <jndi:binding name="sad/sad.groups.enabled">
                    <jndi:value type="java.lang.String">false</jndi:value>
                </jndi:binding>

                <!-- Habilita o deshabilita las funcionadlidades asociadas al Sistema 
                    de Agenda Centralizado -->
                <jndi:binding name="sad/sad.sac.enabled">
                    <jndi:value type="java.lang.String">true</jndi:value>
                </jndi:binding>

                <!-- Directorio donde se almacenan los logs -->
                <jndi:binding name="sad/sad.log.directory">
                    <jndi:value type="java.lang.String">/opt/jboss/server/default/log</jndi:value>
                </jndi:binding>

                <!-- Propiedades de backup -->
                <!-- Directorio donde se almacenan los backups de datos PIM de las entidades 
                    existentes en un SAD -->
                <jndi:binding name="sad/sad.backup.directory">
                    <jndi:value type="java.lang.String">/var/backups/sad</jndi:value>
                </jndi:binding>

                <!-- Intervalo de tiempo por defecto para las copias de seguridad de 
                    los datos de usuario -->
                <jndi:binding name="sad/sad.backup.interval">
                    <jndi:value type="java.lang.String">24</jndi:value>
                </jndi:binding>

                <!-- Indicador de rotación de dichos backups (en días) -->
                <jndi:binding name="sad/sad.backup.rotation">
                    <jndi:value type="java.lang.String">5</jndi:value>
                </jndi:binding>

                <!-- Directorio en el que se guardara el fichero execution.conf que configura 
                    el proceso de backup. Este fichero no se debe tocar de forma manual y se 
                    crea automáticamente si no existe -->
                <jndi:binding name="sad/sad.backup.config.directory">
                    <jndi:value type="java.lang.String">/var/backups/sad/config</jndi:value>
                </jndi:binding>

                <!-- Hora de comienzo del proceso de backup para los backups de 24 horas. 
                    Debe ser un numero entre 0 y 23 -->
                <jndi:binding name="sad/sad.backup.executionHour">
                    <jndi:value type="java.lang.String">0</jndi:value>
                </jndi:binding>


                <!-- Propiedades del servidor SMTP -->
                <!-- Host de la pasarela SMTP de correo electrónico -->
                <jndi:binding name="sad/smtp.host">
                    <jndi:value type="java.lang.String">10.240.202.110</jndi:value>
                </jndi:binding>

                <!-- Puerto de la pasarela SMTP de correo electrónico -->
                <jndi:binding name="sad/smtp.port">
                    <jndi:value type="java.lang.String">25</jndi:value>
                </jndi:binding>

                <!-- Usuario para enviar mensajes a través de la pasarela SMTP de correo 
                    electrónico (si el servicio está securizado) -->
                <jndi:binding name="sad/smtp.user">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Password de dicho usuario -->
                <jndi:binding name="sad/smtp.password">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>


                <!-- Propiedades del CSA -->
                <!-- URL base del CSA -->
                <jndi:binding name="sad/csa.base.url">
                    <jndi:value type="java.lang.String">http://10.240.202.23</jndi:value>
                </jndi:binding>

                <!-- URL al servicio web de adminstración del CSA -->
                <jndi:binding name="sad/csa.ws.admin.user.url">
                    <jndi:value type="java.lang.String">http://10.240.202.23/soap/user_management/</jndi:value>
                </jndi:binding>

                <!-- Usuario para acceder al servicio web de administración del CSA. 
                    Este usuario debe ser proporcionado por CEIC -->
                <jndi:binding name="sad/csa.ws.admin.user.username">
                    <jndi:value type="java.lang.String">csa-ws</jndi:value>
                </jndi:binding>

                <!-- Contraseña del usuario para acceder al servicio web de administración 
                    del CSA. Este usuario debe ser proporcionado por CEIC -->
                <jndi:binding name="sad/csa.ws.admin.user.password">
                    <jndi:value type="java.lang.String">agatav2</jndi:value>
                </jndi:binding>

                <!-- URL del proxy HTTP (si existe) que usará el cliente que accede al 
                    servicio web de administración del CSA -->
                <jndi:binding name="sad/csa.ws.admin.user.proxy">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Puerto de dicho proxy -->
                <jndi:binding name="sad/csa.ws.admin.user.proxyPort">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Usuario para utilizar dicho proxy -->
                <jndi:binding name="sad/csa.ws.admin.user.proxyUser">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Contraseña de dicho usuario -->
                <jndi:binding name="sad/csa.ws.admin.user.proxyPass">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>


                <!-- Propiedades de GUIA -->
                <!-- URL de los web services de GUIA -->
                <jndi:binding name="sad/guia.ws.url">
                    <jndi:value type="java.lang.String">https://guia.ves.i-administracion.junta-andalucia.es/servicio/wsdirectorio/directorio</jndi:value>
                </jndi:binding>

                <!-- Usuario de acceso a los web services -->
                <jndi:binding name="sad/guia.ws.username">
                    <jndi:value type="java.lang.String">cn=cs_agat_001,o=servicios,o=identidades,o=comun,dc=juntadeandalucia,dc=es</jndi:value>
                </jndi:binding>

                <!-- Contraseña -->
                <jndi:binding name="sad/guia.ws.password">
                    <jndi:value type="java.lang.String">Ag@ta10_int</jndi:value>
                </jndi:binding>

                <!-- Proxy a GUIA, en caso de que exista -->
                <jndi:binding name="sad/guia.ws.proxy">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Puerto del proxy -->
                <jndi:binding name="sad/guia.ws.proxyPort">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Usuario, en caso de que el proxy lo requiera -->
                <jndi:binding name="sad/guia.ws.proxyUser">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Contraseña del proxy -->
                <jndi:binding name="sad/guia.ws.proxyPass">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>

                <!-- Ubicación del repositorio de Axis2. Es de uso interno. Dejar siempre 
                    vacío -->
                <jndi:binding name="sad/guia.ws.repoLocation">
                    <jndi:value type="java.lang.String"></jndi:value>
                </jndi:binding>


                <!-- URL de la página de ayuda del SAD -->
                <jndi:binding name="sad/sad.support.help.url">
                    <jndi:value type="java.lang.String">http://10.240.202.23:8080/doc</jndi:value>
                </jndi:binding>


                <!-- URL de acceso a la interfaz gráfica de usuario del SAD -->
                <jndi:binding name="sad/sad.gui.url">
                    <jndi:value type="java.lang.String">http://10.240.202.23:8080/sad-web/</jndi:value>
                </jndi:binding>


                <!-- Asunto de los correos de notificacion de activacion de cuenta -->
                <jndi:binding name="sad/mail.enable.user.notification.subject">
                    <jndi:value type="java.lang.String">Cuenta activada en Ag@ta</jndi:value>
                </jndi:binding>

                <!-- Asunto de los correos de notificacion de desactivacion de cuenta -->
                <jndi:binding name="sad/mail.disable.user.notification.subject">
